extends layout

block scripts
  script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
  script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.2.12/angular.min.js')
  script(src='/scripts/admin.js')

block content
  .container(ng-app='admin',ng-controller='main',ng-init="loadConfig()")
    h3 Mud Config
    div
      textarea(style='width:90%;height:500px;',ng-model="mudconfigJson")
    button.btn.btn-primary(ng-click='saveConfig()') Save
